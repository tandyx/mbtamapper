{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "firefox",
            "type": "firefox",
            "request": "launch",
            "url": "http://localhost:5000",
            "pathMappings": [
                {
                    "url": "http://localhost:5000/*",
                    "path": "${workspaceFolder}"
                }
            ]
        }
    ]
}